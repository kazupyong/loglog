<div class="row">
  <div class="col-lg-12">
    <div class="page-header">
      <h1 id="tables">LogLog Management Screen</h1>
    </div>
    <%= render :partial => "/shared/alert.erb" %>
  </div>
</div>
<div class="row">
  <div class="col-lg-3">
  </div>


  <div class="panel panel-primary col-lg-6">
    <div class="panel-heading">
      <h3 class="panel-title">Login</h3>
    </div>
    <div class="panel-body">
      <%= form_for(resource, as: resource_name, url: session_path(resource_name),:html => {:class => "form-horizontal"}) do |f| %>
          <fieldset>
            <div class="form-group">
              <label for="inputEmail" class="col-lg-2 control-label"><%= f.label :email %></label>
              <div class="col-lg-10">
                <%= f.email_field :email, autofocus: true , :class => "form-control", :id=> "inputEmail" %>
              </div>
            </div>
            <div class="form-group">
              <label for="inputPassword" class="col-lg-2 control-label"><%= f.label :password %></label>
              <div class="col-lg-10">
                <%= f.password_field :password, autocomplete: "off" , :class => "form-control", :id=> "inputPassword" %>
                <div class="checkbox">
                  <label>
                    <% if devise_mapping.rememberable? -%>
                        <%= f.check_box :remember_me %> <%= f.label :remember_me %>
                    <% end -%>
                  </label>
                </div>
              </div>
            </div>
            <div class="form-group">
              <div class="col-lg-10 col-lg-offset-2">
                <%= f.submit "Sign in" , :class => "btn btn-primary" %>
              </div>
            </div>
          </fieldset>
      <% end %>
    </div>
  </div>

  <div class="col-lg-3">
  </div>
</div>
<%= render "devise/shared/links" %>